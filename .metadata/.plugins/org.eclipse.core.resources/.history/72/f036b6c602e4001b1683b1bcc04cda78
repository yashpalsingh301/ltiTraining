import java.util.*;
public class Ch_91 {

	/**
	 * @param args
	 */
	public static void main(String[] args) {
		
		int[] a= {1,2,3,4};
		System.out.println(a.length);
		
		ArrayList<Integer> l1= new ArrayList<>();
		l1.add(12);
		l1.add(14);
		l1.add(34);
		for (int i=0; i<l1.size(); i++){
			System.out.println(l1.get(i));
		}
		

	}

}
